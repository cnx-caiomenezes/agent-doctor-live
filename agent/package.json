{
  "name": "@conexasaude/agent",
  "description": "Conexa Health TeleMedicine Live",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "author": {
    "name": "Caio Menezes",
    "email": "caio.menezes@conexasaude.com.br",
    "url": "https://www.conexasaude.com.br"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=25.0.0",
    "npm": ">=11.6.2",
    "pnpm": ">=10.0.0"
  },
  "keywords": [
    "livekit",
    "voice-agent",
    "telemedicine",
    "healthcare",
    "ai",
    "ai-agent",
    "speech-to-text",
    "stt",
    "transcription",
    "recording",
    "egress",
    "real-time",
    "javascript",
    "typescript",
    "nodejs",
    "agents",
    "openai",
    "cartesia",
    "deepgram",
    "silero"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{ts,js}\"",
    "lint:fix": "eslint --fix \"**/*.{ts,js}\"",
    "format": "prettier . --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier . --check \"**/*.{ts,js,json,md}\"",
    "test": "node --test --import tsx",
    "test:watch": "node --test --watch --import tsx",
    "dev": "tsx watch src/main.ts dev",
    "download-files": "pnpm run build && node dist/main.js download-files",
    "start": "node dist/main.js start"
  },
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/env": "^5.0.3",
    "@fastify/jwt": "^10.0.0",
    "@fastify/sensible": "^6.0.3",
    "@livekit/agents": "^1.0.15",
    "@livekit/agents-plugin-deepgram": "~1.0.15",
    "@livekit/agents-plugin-livekit": "^1.0.15",
    "@livekit/agents-plugin-openai": "~1.0.15",
    "@livekit/agents-plugin-silero": "^1.0.15",
    "@livekit/noise-cancellation-node": "^0.1.9",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "livekit-server-sdk": "^2.14.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.9.2",
    "eslint": "^9.39.0",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "prettier": "3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  }
}