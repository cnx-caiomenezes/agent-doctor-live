# Server Configuration
PORT=3003
NODE_ENV=development
LOG_LEVEL=debug

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/agent_doctor_live

# Redis
REDIS_URL=redis://localhost:6379
REDIS_KEY_PREFIX=agent:

# LiveKit Integration
LIVEKIT_URL=ws://localhost:7880
LIVEKIT_API_KEY=devkey
LIVEKIT_API_SECRET=secret

# OpenAI Configuration
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=500

# Embedding Configuration (Vector Search)
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_EMBEDDING_DIMENSIONS=768
VECTOR_SIMILARITY_THRESHOLD=0.7
VECTOR_TOP_K_RESULTS=5

# Agent Behavior
AGENT_AUTO_JOIN=true
AGENT_MAX_CONCURRENT_SESSIONS=10
AGENT_IDENTITY=ai-agent
AGENT_NAME=Assistente IA Conexa